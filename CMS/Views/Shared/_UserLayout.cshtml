<body>

	<div class="layer"></div>
	<!-- Mobile menu overlay mask -->



	<header class="header_sticky">
		<div class="container">
			<div class="row">
				<div class="col-lg-3 col-6">
					<div id="logo_home"> 
						<h1><a asp-action="Index" asp-controller="Home" title="Providence CLinic">Providence Clinic</a></h1>
					</div>
				</div>
				<nav class="col-lg-9 col-6">
					<a class="cmn-toggle-switch cmn-toggle-switch__htx open_close" href="#0"><span>Menu mobile</span></a>
					<ul id="top_access">
						@if (TempData["Name"] != null)
						{
							<li><a asp-action="Cart" asp-controller="Home"><i class="pe-7s-cart"></i></a></li>
						}
						else
						{
							<li><a asp-action="Login" asp-controller="Home"><i class="pe-7s-cart"></i></a></li>

						}

					</ul>
					<div class="main-menu">

						<ul>
							
							<li class="submenu">
								<a asp-action="Index" asp-controller="Home" class="show-submenu">Home</a>

							</li>
				
							
							<li><a asp-action="Aboutus" asp-controller="Home">About us</a></li>
							<li><a asp-action="Contactus" asp-controller="Home">Contact us</a></li>
							
							<li class="submenu">
								<a asp-action="ProductsPage" asp-controller="Home" asp-route-category="All" class="show-submenu">Products<i class="icon-down-open-mini"></i></a>
								<ul>
									<li><a asp-action="ProductsPage" asp-controller="Home" asp-route-category="scientific">Scientific Machinery</a></li>
									<li><a asp-controller="Home" asp-action="ProductsPage" asp-route-category="medicine">Medicines</a></li>
								
								</ul>
							</li>
							<li><a asp-action="ShowEvents" asp-controller="Home">Events</a></li>
							

							@if (TempData["e_id"] != null)
							{
								<li><a asp-action="EventPass" asp-controller="Home">Pass</a></li>
								
							}
							@if (TempData["Name"] == null)
							{
								
								<li><a asp-action="Register" asp-controller="Home">Register</a></li>
							}
							else
							{
								<li><a asp-action="Logout" asp-controller="Home">Logout</a></li>
							}
						</ul>
					</div>
					<!-- /main-menu -->
				</nav>
			</div>
		</div>
		<!-- /container -->
	</header>
	<!-- /header -->
	<section>
		@RenderBody()
